        "rules": [
          {
            "description": "Launcher Mode",
            "manipulators": [
              {
                "from": {
                  "key_code": "w"
                },
                "to_after_key_up": [
                  {
                    "set_variable": {
                      "name": "launcher_mode",
                      "value": 0
                    }
                  }
                ],
                "to_if_alone": [
                  {
                    "key_code": "w"
                  }
                ],
                "to_if_held_down": [
                  {
                    "set_variable": {
                      "name": "launcher_mode",
                      "value": 1
                    }
                  }
                ],
                "type": "basic"
              },
              {"conditions": [{"name": "launcher_mode","type": "variable_if","value": 1}],
                "from": {"key_code": "i"},
                "to": [{"shell_command": "open '/Applications/iTerm.app'"}],
                "type": "basic"
              },
              {"conditions": [{"name": "launcher_mode","type": "variable_if","value": 1}],
                "from": {"key_code": "o"},
