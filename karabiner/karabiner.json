                "type": "basic"
              },
              {"conditions": [{"name": "launcher_mode","type": "variable_if","value": 1}],
                "from": {"key_code": "s"},
                "to": [{"shell_command": "open '/Applications/Simplenote.app'"}],
                "type": "basic"
              },
              {"conditions": [{"name": "launcher_mode","type": "variable_if","value": 1}],
                "from": {"key_code": "p"},
                "to": [{"shell_command": "open '/Applications/Spotify.app'"}],
                "type": "basic"
              }
            ]
          },
